# A configuration in which each axis has a different number
# of steps/mm. This helps in identifying if there is trouble
# because of different handling of these.
[ X-Axis ]
steps-per-mm     =  100
max-feedrate     =  300
max-acceleration =  200
home-pos         = min
range = 500

[ Y-Axis ]
steps-per-mm     = 1000
max-feedrate     =  300
max-acceleration =  200
home-pos         = min
range = 500

[ Z-Axis ]
steps-per-mm     = 10000
max-feedrate     =   4
max-acceleration = 100
home-pos         = max
range = 100
